<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.outbackexmo.mybudgetmanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ExpenseList" />
        <activity android:name=".DetailExpense" />
        <activity android:name=".SearchExpense" />
        <activity android:name=".StaticExpense" />
        <activity android:name=".AddItem" />
        <activity android:name=".BudgetSetting" />
        <activity android:name=".DetailItem" />
        <activity android:name=".DetailItemList"></activity>
    </application>

</manifest>